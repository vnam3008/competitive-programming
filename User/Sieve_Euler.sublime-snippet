	<content><![CDATA[
vector <int> phi(LIM);
for (int i = 0; i < LIM; i++) phi[i] = i;
for (int i = 2; i*i < LIM; i++)
    if (phi[i] == i)
        for (int j = i; i*j < LIM; j++) phi[i*j] = i;
]]></content>
